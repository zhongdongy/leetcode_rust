  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'leetcode_rust'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=leetcode_rust"
        ],
        "filter": {
          "name": "leetcode_rust",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug integration test 'problems'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--package=leetcode_rust"
        ],
        "filter": {
          "name": "problems",
          "kind": "test"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}